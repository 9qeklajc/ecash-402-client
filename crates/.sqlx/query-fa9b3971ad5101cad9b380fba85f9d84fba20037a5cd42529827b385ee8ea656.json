{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            t.id,\n            t.created_at,\n            t.token,\n            t.amount,\n            t.direction as \"direction: TransactionDirection\",\n            t.api_key_id::text,\n            t.user_id::text,\n            t.type as \"type: TransactionType\"\n        FROM transactions t\n        LEFT JOIN api_keys ak ON t.api_key_id = ak.id\n        WHERE t.user_id = $1 OR ak.organization_id = $2\n        ORDER BY t.created_at DESC\n        LIMIT $3 OFFSET $4\n        ",
